@article{wang_phase_2014,
	title = {Phase Transitions of Ferromagnetic Potts Models on the Simple Cubic Lattice},
	url = {http://arxiv.org/abs/1405.1179},
	abstract = {We investigate the 2- and 3-state ferromagnetic Potts models on the simple cubic lattice using the tensor renormalization group method with higher-order singular value decomposition ({HOTRG}). {HOTRG} works in the thermodynamic limit, where we use the \$Z\_q\$ symmetry of the model, combined with a new measure for detecting the transition, to improve the accuracy of the critical point for the 2-state model by two orders of magnitude, obtaining \$T\_c = 4.51152469(1)\$. The 3-state model is far more complex, and we improve the overall understanding of this case by calculating its thermodynamic quantities with high accuracy. Our results verify the first-order nature of the phase transition and the {HOTRG} transition temperature benchmarks the most recent Monte Carlo result.},
